<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pda6100_scan_ver00">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".MyApplication">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.pda6100_scan_ver00.Setting" />
        <activity android:name="com.example.pda6100_scan_ver00.ScanGroup"/>
        <activity android:name="com.example.pda6100_scan_ver00.Scan"/>
        <activity android:name="com.example.pda6100_scan_ver00.DataManage"/>
    </application>

</manifest>